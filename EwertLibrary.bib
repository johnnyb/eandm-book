keywords = {Biological Evolution,Mutation,Pseudomonas fluorescens,Pseudomonas fluorescens: genetics,Pseudomonas fluorescens: physiology,beneficial-mutation-rate},
mendeley-tags = {beneficial-mutation-rate},
